    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "4.3.2"
  },
  "lint-staged": {
    "src/**/*.ts{,x}": [
      "eslint --fix",
      "prettier --write"
    ]
